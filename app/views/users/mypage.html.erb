
<div class=container>
    <% @selectstore.each do |member| %>
        <div class="row py-3">
            <div class="col align-self-center">
                <div class="border rounded shadow p-3 mb-5 bg-white rounded" style="padding:10px;"> 
                    <%= member.name %>
                    <div>
                        <% if member.logo %>
                            <%=image_tag member.logo %>
                        <% end %>
                    </div>
                    <div>
                    <%=link_to "詳細ページ", member.url %>
                    </div>
                </div>
            </div>
        </div>
    <% end %>
</div>
