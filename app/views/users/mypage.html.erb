
<div class=container>
    <div class="row py-3">
        <div class="col align-self-center">
            <div class="border rounded shadow p-3 mb-5 bg-white rounded" style="padding:10px;"> 
                <% @selectstore.each do |member| %>
                        <%= member.store_id %><br>
                        <%= member.name %><br>
                        <% if member.logo %>
                            <%=image_tag member.logo %><br>
                        <% end %>
                        <%=link_to "詳細ページ", member.url %><br>
                <% end %>
            </div>
        </div>
    </div>
</div>
